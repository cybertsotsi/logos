		if(index != 0){

			var selected = options[index];
			var array = new Array();
			array=selected.value.split(",");
			var activity = array[0];
			var step = array[1];
			var active = array[2];
			var id = array[3];
			document.getElementById('cat').value = activity;
			if(step == 'null' || step == 'Null' || step == 'NULL'){
				document.getElementById('step').value = '';	
			}
			else{
				document.getElementById('step').value = step;
			}
			if(active == 'true'){
				document.getElementById('active').checked = true;
				document.getElementById('active').value = 'on';
			}	
			else{
				document.getElementById('active').checked = false;
				document.getElementById('active').value = 'off';
			}
			document.getElementById('flood_id').value = id;
		}
		else{
			document.getElementById('cat').value='';
			document.getElementById('step').value='';
			document.getElementById('type').value='';
			document.getElementById('active').checked = false;
			document.getElementById('flood_id').value='';
		}
		
	}

